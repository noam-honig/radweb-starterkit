{
  "name": "radweb-starterkit",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "ng-serve": "ng serve",
    "start": "npm run server:execute",
    "start_old": "ts-node --project tsconfig.server.json src/app/server/server.ts",
    "start_dev": "concurrently \"ng serve\" \"npm run server:watch\"",
    "build": "ng build --prod",
    "postinstall": "npm run server:build&&npm run build",
    "publish-to-heroku": "git push heroku master",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "server:build": "tsc -p tsconfig.server.json",
    "server:execute": "node dist-server/src/app/server/server.js",
    "server:start": "npm run server:build && npm run server:execute",
    "server:watch": "concurrently \"tsc -P tsconfig.server.json -w\" \"nodemon dist-server/src/app/server/server.js --ignore /appData --ignore /dist\""
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^5.2.0",
    "@angular/common": "^5.2.0",
    "@angular/compiler": "^5.2.0",
    "@angular/core": "^5.2.0",
    "@angular/forms": "^5.2.0",
    "@angular/http": "^5.2.0",
    "@angular/platform-browser": "^5.2.0",
    "@angular/platform-browser-dynamic": "^5.2.0",
    "@angular/router": "^5.2.0",
    "@types/body-parser": "^1.16.8",
    "@types/express": "^4.11.0",
    "@types/mssql": "^4.0.6",
    "body-parser": "^1.18.2",
    "bootstrap3": "^3.3.5",
    "core-js": "^2.4.1",
    "dotenv": "^5.0.1",
    "express": "^4.16.2",
    "jquery": "^3.3.1",
    "mssql": "^4.1.0",
    "ngx-bootstrap": "^2.0.3",
    "path": "^0.12.7",
    "pg": "^7.4.1",
    "radweb": "^1.1.16",
    "rxjs": "^5.5.6",
    "zone.js": "^0.8.19"
  },
  "devDependencies": {
    "@angular/cli": "~1.7.3",
    "@angular/compiler-cli": "^5.2.0",
    "@angular/language-service": "^5.2.0",
    "@types/dotenv": "^4.0.2",
    "@types/jasmine": "~2.8.3",
    "@types/jasminewd2": "~2.0.2",
    "@types/jquery": "^3.2.16",
    "@types/node": "~6.0.60",
    "@types/pg": "^7.4.5",
    "codelyzer": "^4.0.1",
    "concurrently": "^3.5.1",
    "jasmine-core": "~2.8.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~6.3.14",
    "karma-chrome-launcher": "~2.2.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "ngrok": "^2.2.23",
    "nodemon": "^1.14.3",
    "protractor": "~5.1.2",
    "ts-node": "^4.1.0",
    "tslint": "~5.9.1",
    "typescript": "~2.5.3"
  }
}
